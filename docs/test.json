{
    "basePath": "http://localhost:8000",
    "swaggerVersion": "1.2",
    "apiVersion": "1.0.0",
    "resourcePath": "/test",
    "apis": [
        {
            "path": "/test",
            "description": "Operations about pets",
            "operations": [
                {
                    "method": "GET",
                    "summary": "Find pet by ID",
                    "nickname": "getPetById",
                    "type": "Pet",
                    "parameters": [
                        {
                            "paramType": "path",
                            "name": "petId",
                            "type": "string",
                            "description": "ID of pet that needs to be fetched"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 400,
                            "message": "Invalid ID supplied"
                        },
                        {
                            "code": 404,
                            "message": "Pet not found"
                        }
                    ],
                    "notes": "Returns a pet based on ID"
                }
            ]
        }
    ],
    "models": {
        "Pet": {
            "id": "Pet",
            "properties": {
                "tags": {
                    "type": "array",
                    "items": {
                        "$ref": "Tag"
                    }
                }
            }
        }
    },
    "produces": [
        "application/json",
        "application/xml",
        "text/plain",
        "text/html"
    ]
}