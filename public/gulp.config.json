{
  "build": {
    "folder": "public/build/",
    "files": {
      "js": "app.js",
      "css": "app.css",
      "vendorjs": "vendor.js",
      "vendorcss": "vendor.css"
    }
  },
  "spec": [
    "public/app/**/*.spec.js"
  ],
  "js": [
    "public/app/js/**/*.module.js",
    "public/app/js/**/*.config.js",
    "public/app/js/**/*.constant.js",
    "public/app/js/**/*.controller.js",
    "public/app/js/**/*.filter.js",
    "public/app/js/**/*.factory.js",
    "public/app/js/**/*.directive.js",
    "public/app/js/**/*.provider.js"
  ],
  "less": [
    "public/css/jp-styling/main.less"
  ],
  "vendorjs": [
    "public/bower_components/jqueryui/jquery-ui.js",
    "public/bower_components/jquery/dist/jquery.js",
    "public/bower_components/angular/angular.js",
    "public/bower_components/angular-ui-router/release/angular-ui-router.js",
    "public/bower_components/toastr/toastr.js",

  ],
  "vendorcss": [
    "public/bower_components/ng-tags-input/ng-tags-input.bootstrap.css",
    "public/bower_components/ng-tags-input/ng-tags-input.css",
    "public/bower_components/toastr/toastr.css"
  ],
  "testing": [
    "public/bower_components/angular/angular.js",
    "public/bower_components/angular-mocks/angular-mocks.js"
  ]

}